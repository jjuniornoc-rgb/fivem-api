{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "docs/api",
  "name": "varlet-fivem-api",
  "includeVersion": true,
  "readme": "README.md",
  "exclude": ["**/*.test.ts", "**/__tests__/**", "node_modules"],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "defaultCategory": "Outros",
  "categoryOrder": ["Classes", "Funções", "Tipos", "Outros"],
  "searchInComments": true,
  "customCss": "typedoc-theme/custom.css",
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "plugin": []
}
